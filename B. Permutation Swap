#include <bits/stdc++.h>
using namespace std;
#define ll long long
#pragma GCC optimize("O3,unroll-loops")
 
 int gcd(int x, int y){
   if(x==0){
     return y;
   }
   return gcd(y%x,x);
 }
 
 
void solve(){
  
  int n;
  cin>>n;
  
  vector<int>a(n);
  for(auto&k:a){
    cin>>k;
    
  }
 
 vector<int>str;
 
 for(int i=0; i<n; i++){
  str.push_back(abs(a[i] - (i + 1)));
 }
 sort(str.begin(),str.end());
 int res=str[0];
 for(int i=1; i<str.size(); i++){
    res=gcd(str[i],res);
    if(res==1){
      cout<<1<<endl;
      return;
    }
 }
 cout<<res<<endl;
  
  
  
}
 
signed main() {
  
   ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin>>t;
    while(t--)
   {
      
      solve ();
    }
    return 0;
} 
