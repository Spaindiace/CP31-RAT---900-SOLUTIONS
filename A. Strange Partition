#include <bits/stdc++.h>
using namespace std;
#define ll long long
#pragma GCC optimize("O3,unroll-loops")
 
void solve(){
  
 int n,x;
 cin>>n>>x;
 
 vector<int>a(n);
 ll sum=0;
 for(auto&k:a){
   cin>>k;
   sum+=k;
 }
 
 ll maxi=0;
 for(int i=0; i<n; i++){
     maxi+=(a[i]+x-1)/x;
 }
 
 cout<<(sum+x-1)/x<<" "<<maxi<<endl;

 
 
 
        
}
 
signed main() {
  
   ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin>>t;
    while(t--)
   {
      
      solve ();
    }
    return 0;
} 
